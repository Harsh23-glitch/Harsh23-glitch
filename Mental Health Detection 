      "cell_type": "code",
      "source": [
        "tokenizer = BertTokenizer.from_pretrained(\"bert-base-uncased\")\n",
        "bert = BertModel.from_pretrained(\"bert-base-uncased\")\n"
      ],
      "metadata": {
        "id": "ibK1XMmade3m"
      },
      "execution_count": 91,
      "outputs": []
