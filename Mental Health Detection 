        "\n",
        "encoding = tokenizer(text, return_tensors=\"pt\", padding=True, truncation=True, max_length=128)\n",
        "\n",
        "with torch.no_grad():\n",
        "    logits = model(encoding['input_ids'], encoding['attention_mask'])\n",
        "\n",
        "logits\n"
