    {
      "cell_type": "code",
      "source": [
        "import torch\n",
        "import torch.nn.functional as F\n",
        "import gradio as gr\n",
        "\n",
        "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
        "model.to(device)\n",
        "model.eval()\n",
        "\n",
        "id_to_label = {0: \"ANXIETY\", 1: \"DEPRESSION\", 2: \"NORMAL\"}\n",
        "\n",
        "def predict_mental_state(text):\n",
        "    text_clean = text.lower().strip()\n",
        "\n",
        "    # ---------------- 1 Suicide / self-harm emergency detection ----------------\n",
        "    suicide_words = [\n",
        "        \"kill myself\", \"end my life\", \"i want to die\", \"i want death\",\n",
        "        \"i don't want to live\", \"i dont want to live\", \"suicide\",\n",
        "        \"self harm\", \"self-harm\", \"cut myself\", \"hurt myself\",\n",
        "        \"end it all\", \"die soon\", \"thinking of dying\", \"no reason to live\",\n",
        "        \"life is not worth living\", \"tired of living\", \"i hate living\",\n",
        "        \"i hate my life\", \"erase myself\", \"disappear forever\",\n",
        "        \"better if i was gone\", \"nobody would miss me\", \"give up on life\",\n",
        "        \"planning to die\", \"exit life\", \"delete myself\",\n",
        "        \"can't live anymore\", \"want to sleep forever\", \"wish i was dead\"\n",
        "    ]\n",
        "    for phrase in suicide_words:\n",
        "        if phrase in text_clean:\n",
        "            return (\n",
        "                \" **EMERGENCY SIGNAL DETECTED**\\n\"\n",
        "                \" Possible **SELF-HARM / SUICIDAL THOUGHTS**\\n\"\n",
        "                \" Immediate emotional support is strongly recommended.\"\n",
        "            )\n",
        "\n",
        "    # ---------------- 2 Anxiety override ----------------\n",
        "    anxiety_keywords = [\n",
        "        \"anxious\", \"anxiety\", \"worried\", \"worry\", \"stress\", \"stressed\",\n",
        "        \"panic\", \"panicking\", \"panic attack\", \"fear\", \"afraid\", \"scared\",\n",
        "        \"nervous\", \"shaking\", \"restless\", \"on edge\", \"uneasy\",\n",
        "        \"overthinking\", \"pressure\", \"paranoid\", \"jittery\",\n",
        "        \"can't relax\", \"cant relax\", \"can't calm down\", \"cant calm down\",\n",
        "        \"heartbeat fast\", \"heart racing\", \"my chest feels tight\",\n",
        "        \"mind racing\", \"social anxiety\", \"fear of people\", \"fear of failure\",\n",
        "        \"can't stop thinking\", \"can't focus\", \"too scared to talk\",\n",
        "        \"i doubt myself\", \"scared to make decisions\"\n",
        "    ]\n",
        "    for word in anxiety_keywords:\n",
        "        if word in text_clean:\n",
        "            return (\n",
        "                \" Predicted Mental State: **ANXIETY**\\n\"\n",
        "                \" Auto-correction triggered due to strong anxiety indicators\"\n",
        "            )\n",
        "\n",
        "    # ---------------- 3 Positivity override (Normal) ----------------\n",
        "    positive_keywords = [\n",
        "        \"happy\", \"very happy\", \"so happy\", \"truly happy\", \"really happy\",\n",
        "        \"grateful\", \"thankful\", \"blessed\", \"excited\", \"satisfied\",\n",
        "        \"peaceful\", \"calm\", \"hopeful\", \"optimistic\", \"motivated\",\n",
        "        \"confident\", \"proud of myself\", \"loving my life\", \"love my life\",\n",
        "        \"love myself\", \"self love\", \"enjoy my life\", \"feeling good\",\n",
        "        \"feel good\", \"i'm okay\", \"doing great\", \"great day\",\n",
        "        \"positive mindset\", \"positive energy\", \"smiling a lot\",\n",
        "        \"mentally strong\", \"emotionally strong\", \"feeling amazing\",\n",
        "        \"life is good\", \"i appreciate life\", \"i like who i am\",\n",
        "        \"i feel safe\", \"feeling stable\", \"i am calm inside\"\n",
        "    ]\n",
        "    for word in positive_keywords:\n",
        "        if word in text_clean:\n",
        "            return (\n",
        "                \" Predicted Mental State: **NORMAL**\\n\"\n",
        "                \" Auto-correction triggered due to positive emotional indicators\"\n",
        "            )\n",
        "\n",
        "    # ---------------- 4 Default BERT-LSTM model prediction ----------------\n",
        "    encoding = tokenizer(\n",
        "        text, return_tensors=\"pt\", truncation=True, padding=True, max_length=128\n",
        "    )\n",
        "    input_ids = encoding[\"input_ids\"].to(device)\n",
        "    attention_mask = encoding[\"attention_mask\"].to(device)\n",
        "\n",
        "    with torch.no_grad():\n",
        "        logits = model(input_ids, attention_mask)\n",
        "        probs = F.softmax(logits, dim=-1)[0].cpu().numpy()\n",
        "\n",
        "    pred_id = int(probs.argmax())\n",
        "    pred_label = id_to_label.get(pred_id, \"UNKNOWN\")\n",
        "\n",
        "    return (\n",
        "        f\" Predicted Mental State: **{pred_label}**\\n\\n\"\n",
        "        f\" Class Probabilities:\\n\"\n",
        "        f\"- ANXIETY: {probs[0]*100:.2f}%\\n\"\n",
        "        f\"- DEPRESSION: {probs[1]*100:.2f}%\\n\"\n",
        "        f\"- NORMAL: {probs[2]*100:.2f}%\"\n",
        "    )\n",
        "\n",
        "# ---------------- Gradio UI ----------------\n",
        "ui = gr.Interface(\n",
        "    fn=predict_mental_state,\n",
        "    inputs=gr.Textbox(lines=5, placeholder=\"Type what you're feeling...\"),\n",
        "    outputs=\"markdown\",\n",
        "    title=\" Mental Health Detection (BERT + LSTM + Safety Overrides)\",\n",
        "    description=\"Classifies text as ANXIETY, DEPRESSION, or NORMAL  with emergency detection for self-harm or suicide risk.\"\n",
        ")\n",
        "\n",
        "ui.launch(share=True)\n"
