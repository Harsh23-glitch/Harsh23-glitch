    {
      "cell_type": "code",
      "source": [
        "class BERT_LSTM(nn.Module):\n",
        "    def __init__(self, dropout=0.3):\n",
        "        super(BERT_LSTM, self).__init__()\n",
        "\n",
        "        self.bert = BertModel.from_pretrained(\"bert-base-uncased\")\n",
        "\n",
        "        self.lstm = nn.LSTM(\n",
        "            input_size=768,\n",
        "            hidden_size=256,\n",
        "            num_layers=1,\n",
        "            batch_first=True,\n",
        "            bidirectional=True\n",
        "        )\n",
        "\n",
        "        self.fc = nn.Linear(512, 3)  # 3 classes: anxiety, depression, normal\n",
        "        self.dropout = nn.Dropout(dropout)\n",
        "\n",
        "    def forward(self, input_ids, attention_mask):\n",
        "        with torch.no_grad():\n",
        "            bert_output = self.bert(input_ids=input_ids, attention_mask=attention_mask)\n",
        "\n",
        "        sequence_output = bert_output.last_hidden_state  # (batch, seq, 768)\n",
        "\n",
        "        lstm_out, _ = self.lstm(sequence_output)\n",
        "\n",
        "        final_hidden_state = lstm_out[:, -1, :]  # last time step\n",
        "\n",
        "        output = self.fc(self.dropout(final_hidden_state))\n",
        "\n",
        "        return output\n"
      ],
      "metadata": {
        "id": "9UD0ROwsdm6R"
      },
      "execution_count": 92,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model = BERT_LSTM()\n"
      ],
      "metadata": {
        "id": "PZDaVVWIdtRX"
      },
      "execution_count": 93,
      "outputs": []
    },
