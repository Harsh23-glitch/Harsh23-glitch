      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import random\n",
        "from google.colab import files\n",
        "\n",
        "random.seed(42)\n",
        "\n",
        "normal_templates = [\n",
        "    \"i am {adj} today\",\"feeling {adj} rn\",\"ngl i feel pretty {adj}\",\n",
        "    \"lowkey {adj} about life\",\"not gonna lie kinda {adj} lately\",\n",
        "    \"mentally i'm feeling {adj}\",\"my mood is {adj} tbh\",\"vibes are kinda {adj} today\",\n",
        "    \"idk i just feel {adj}\",\"right now i'm actually {adj}\"\n",
        "]\n",
        "\n",
        "anxiety_templates = [\n",
        "    \"i feel so {adj} rn\",\"my anxiety be making me {adj}\",\n",
        "    \"ngl i'm kinda {adj} about everything\",\"overthinking again feel super {adj}\",\n",
        "    \"social stuff makes me so {adj}\",\"why am i always this {adj}\",\n",
        "    \"brain won't chill i'm {adj} as hell\",\"i get so {adj} for no reason\",\n",
        "    \"my chest feels tight i'm {adj}\",\"whole day been feeling {adj}\"\n",
        "]\n",
        "\n",
        "depression_templates = [\n",
        "    \"lately i just feel {adj}\",\"ngl i'm so {adj} inside\",\n",
        "    \"mentally i'm just {adj} rn\",\"it's all kinda {adj} these days\",\n",
        "    \"woke up feelin {adj} again\",\"tired of feeling this {adj}\",\n",
        "    \"everything feels {adj} tbh\",\"lowkey been {adj} for a while\",\n",
        "    \"smile outside but {adj} inside\",\"can't lie i'm really {adj} lately\"\n",
        "]\n",
        "\n",
        "normal_adjs = [\"happy\",\"chill\",\"good\",\"great\",\"okay\",\"calm\",\"optimistic\",\"relaxed\",\"motivated\",\"hopeful\"]\n",
        "anxiety_adjs = [\"anxious\",\"nervous\",\"stressed\",\"worried\",\"on edge\",\"restless\",\"panicky\",\"uneasy\",\"jittery\",\"overthinking\"]\n",
        "depression_adjs = [\"empty\",\"numb\",\"broken\",\"lost\",\"hopeless\",\"tired\",\"drained\",\"worthless\",\"sad\",\"alone\"]\n",
        "\n",
        "def generate(count, templates, adjs, label):\n",
        "    data = []\n",
        "    for _ in range(count):\n",
        "        template = random.choice(templates)\n",
        "        adj = random.choice(adjs)\n",
        "        data.append([template.format(adj=adj), label])\n",
        "    return data\n",
        "\n",
        "total = 11000\n",
        "normal_count = total // 3 + 1\n",
        "anxiety_count = total // 3\n",
        "depression_count = total // 3 + 1\n",
        "\n",
        "rows = []\n",
        "rows += generate(normal_count, normal_templates, normal_adjs, \"normal\")\n",
        "rows += generate(anxiety_count, anxiety_templates, anxiety_adjs, \"anxiety\")\n",
        "rows += generate(depression_count, depression_templates, depression_adjs, \"depression\")\n",
        "\n",
        "df = pd.DataFrame(rows, columns=[\"clean_text\", \"label\"])\n",
        "df.to_csv(\"social_mental_health_11000.csv\", index=False)\n",
        "\n",
        "files.download(\"social_mental_health_11000.csv\")\n",
        "print(\"Download ready!\")\n",
        "\n"
      ],
